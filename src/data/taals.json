      "Tin": "Closed bass + closed/ringed treble",
      "Na": "Open stroke on dayan edge",
      "Ti": "Light stroke on dayan",
      "Tun": "Resonant stroke on dayan center",
      "Ka": "Flat palm on bayan",
      "Ke": "Flat palm on bayan (variant)",
      "Ga": "Closed resonant bayan stroke",
      "Ge": "Open resonant bayan stroke",
      "Ghi": "Sliding bayan stroke",
      "Dhi": "Variant of Dhin",
      "Tit": "Light double stroke",
      "Kda": "Compound bol (Ka + Dha)",
      "Trkt": "Tirakita â€“ quick ornamental pattern (Ti Ra Ki Ta)",
      "Dhet": "Open bass + damped treble",
      "Kat": "Sharp closed bol",
      "Gad": "Sliding bayan with resonance",
      "Taa": "Extended open treble",
      "Naa": "Extended edge stroke"
    }
  },
